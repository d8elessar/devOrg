<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Read_Only</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISBLANK(NumberOfEmployees)) &amp;&amp; NOT (ISCHANGED(NumberOfEmployees))</errorConditionFormula>
    <errorMessage>Account is read only.</errorMessage>
</ValidationRule>
